<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lv.poznak.controllers.MainWindowController">
   <children>
      <BorderPane prefHeight="400.0" prefWidth="600.0">
         <top>
            <HBox prefHeight="15.0" prefWidth="600.0" BorderPane.alignment="CENTER">
               <children>
                  <Button mnemonicParsing="false" prefHeight="20.0" prefWidth="52.0" text="Play" onAction="#playMusic" />
                  <Button mnemonicParsing="false" text="Stop" onAction="#stopMusic"/>
                  <Button mnemonicParsing="false" text="Next" onAction="#nextMusic"/>
                  <Button mnemonicParsing="false" text="Prev" onAction="#prevMusic"/>
                  <Label fx:id="musicStatus" prefHeight="25.0" prefWidth="152.0" text="" />
               </children>
            </HBox>
         </top>
         <center>
            <Pane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
         </center>
      </BorderPane>
   </children>
</AnchorPane>
